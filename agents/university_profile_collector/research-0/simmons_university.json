    "strategic_profile": {
      "executive_summary": "Simmons University is a private institution in Boston known for its women-centered undergraduate curriculum and strong professional graduate programs in nursing, social work, and library science. It emphasizes experiential learning and leadership, leveraging its location in a global medical and educational hub.",
      "market_position": "Hidden Gem",
      "admissions_philosophy": "Holistic review",
      "us_news_rank": 165,
      "analyst_takeaways": [
        {
          "category": "Selectivity",
          "insight": "The undergraduate acceptance rate has trended downward from approximately 84% in 2021 to 66% for the 2024-2025 cycle.",
          "implication": "The university is becoming increasingly competitive; applicants should prioritize articulating a strong fit for its women-centered leadership mission."
        },
        {
          "category": "Infrastructure",
          "insight": "The 'One Simmons' project is consolidating the dual-campus model into a single, modernized urban hub including a 21-story Living and Learning Center.",
          "implication": "Prospective students will benefit from significantly upgraded facilities and a more cohesive on-campus social environment."
        },
        {
          "category": "ROI and Value",
          "insight": "Simmons consistently ranks highly for social mobility (#126) and best value (#33).",
          "implication": "The institution is a strong choice for students focused on career-readiness and high graduate outcomes in specialized fields like healthcare and library science."
        }
      ],
      "campus_dynamics": {
        "social_environment": "A small-college feel within a massive student city; students have access to the 'Colleges of the Fenway' (COF) consortium for cross-registration and shared social events.",
        "transportation_impact": "Highly urban and transit-dependent; campus life is shaped by the MBTA 'T' (Green Line) and MASCO shuttles connecting the residential and academic sectors.",
        "research_impact": "Significant research output in healthcare and information sciences, particularly within the School of Library and Information Science (SLIS) which is ranked in the top 10 nationally."
      }
    },
