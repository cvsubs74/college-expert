    "strategic_profile": {
      "executive_summary": "Seattle University is a premier Jesuit Catholic institution located in the heart of Seattle's Capitol Hill neighborhood, emphasizing the education of the whole person and professional formation. The university is distinguished by its commitment to social justice, sustainability, and high-impact undergraduate research within a vibrant urban setting.",
      "market_position": "Elite Private",
      "admissions_philosophy": "Holistic review",
      "us_news_rank": 151,
      "analyst_takeaways": [
        {
          "category": "Selectivity",
          "insight": "Acceptance rates have tightened significantly, dropping from 85.1% in 2022 to approximately 76% in the most recent cycle.",
          "implication": "Prospective students should treat the application as increasingly competitive and leverage the optional supplemental essays to demonstrate alignment with Jesuit values."
        },
        {
          "category": "Value & Mobility",
          "insight": "Ranked #65 nationally for 'Best Value' and recognized as the top private university in Washington for social mobility.",
          "implication": "The school is an attractive option for high-achieving students seeking a high ROI and strong institutional support for diverse backgrounds."
        }
      ],
      "campus_dynamics": {
        "social_environment": "A close-knit, mission-driven community where 180+ student clubs and NCAA Division I athletics coexist with a deep-seated culture of service-learning and political awareness.",
        "transportation_impact": "As an urban campus with a high commuter population, the university integrates life through 'Commuter Link' spaces, subsidized ORCA transit cards, and facilities like commuter showers to maintain a unified campus feel.",
        "research_impact": "Strong emphasis on 'Engaged Student Scholarship' through the Office of Undergraduate Research, which facilitates faculty-mentored projects and hosts the annual Student Research and Creativity Conference (SRCCon)."
      }
    },
