    "strategic_profile": {
      "executive_summary": "The University of St. Thomas is the largest private, nonprofit university in Minnesota, a Catholic research institution with a dual-campus presence in St. Paul and Minneapolis. It is recognized for blending a liberal arts foundation with high-demand professional programs in business, law, and engineering.",
      "market_position": "Elite Private",
      "admissions_philosophy": "Holistic review (Test-optional)",
      "us_news_rank": 158,
      "analyst_takeaways": [
        {
          "category": "Institutional Growth",
          "insight": "The university's transition to NCAA Division I athletics has significantly raised its national profile.",
          "implication": "Prospective students can expect a more high-energy, 'big-school' social atmosphere despite the moderate size."
        },
        {
          "category": "Selectivity",
          "insight": "The acceptance rate remains relatively accessible at approximately 85-91%, yet middle-50% test scores are rising.",
          "implication": "While accessible, merit-based scholarship competition is becoming more rigorous as the school attracts more out-of-state applicants."
        }
      ],
      "campus_dynamics": {
        "social_environment": "A mission-driven atmosphere focused on 'the common good' with a strong emphasis on Catholic intellectual tradition and a vibrant D1 sports culture.",
        "transportation_impact": "An essential inter-campus shuttle connects the traditional, residential St. Paul campus with the professional downtown Minneapolis campus, effectively integrating the Twin Cities urban landscape into daily student life.",
        "research_impact": "Strong undergraduate focus via the Undergraduate Research Opportunities Program (UROP), with significant recent investment in 'STEAM' facilities like the Schoenecker Center to foster industry ties in engineering and data science."
      }
    },
