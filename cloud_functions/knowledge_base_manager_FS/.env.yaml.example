GEMINI_API_KEY: "your_gemini_api_key_here"

# Name of the knowledge base File Search store
DATA_STORE: "college_admissions_kb"

# Chunking configuration for File Search
# Controls how documents are split into chunks for embedding and indexing
# See: https://ai.google.dev/gemini-api/docs/file-search#chunking_configuration
# NOTE: Values must be quoted strings for gcloud deployment
CHUNKING_MAX_TOKENS_PER_CHUNK: "800"
CHUNKING_MAX_OVERLAP_TOKENS: "100"
